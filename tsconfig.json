{
  "compilerOptions": {
    "target": "ES2020",
    "module": "CommonJS", // Altere para CommonJS
    "moduleResolution": "node",
    "baseUrl": ".",
    "outDir": "./dist",
    "esModuleInterop": true,
    "resolveJsonModule": true,
    "strict": true,
    "skipLibCheck": true,
    "paths": {
      "*": ["node_modules/*", "src/*", "config/*"]
    }
  },
  "include": ["src/**/*", "config/**/*"],
  "exclude": ["node_modules"]
}